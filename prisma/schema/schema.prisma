generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

/// 컨플루언스 페이지 항목들
model ConfluencePage {
  id        Int      @id
  title     String
  content   String
  createdAt DateTime @map("created_at")
  updatedAt DateTime @map("updated_at")

  @@map("confluence_page")
}
